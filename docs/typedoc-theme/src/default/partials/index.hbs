{{#if groups}}
<section class="tsd-panel-group tsd-index-group">
    <section class="tsd-panel tsd-index-panel">
        <div class="tsd-index-content">
            <section class="tsd-index-section {{cssClasses}}">
                {{#each groups}}
                <ul class="tsd-index-list">
                    {{#each children}}
                    <li class="{{cssClasses}}"><a href="{{relativeURL url}}" class="tsd-kind-icon">{{#if name}}{{{wbr
                            name}}}{{else}}<em>{{{wbr kindString}}}</em>{{/if}}</a></li>
                    {{/each}}
                </ul>
                {{/each}}
            </section>
        </div>
    </section>
</section>
{{/if}}