<table class="tsd-type-parameters">
    <thead>
        <tr>
            <th>
                Type parameter
            </th>
            <th>
                Description
            </th>
        </tr>
    </thead>
    <tbody>
        {{#each typeParameters}}
        <tr>
            <td class="tsd-type-parameter-name">{{#compact}}
                {{name}}
                {{#if type}}
                <span class="tsd-signature-symbol">:&nbsp;</span>
                {{#with type}}{{> type}}{{/with}}
                {{/if}}
                {{/compact}}
            </td>
            <td>
                {{> comment}}
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>