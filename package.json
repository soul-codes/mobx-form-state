{
  "name": "mobx-ui-statelets",
  "version": "0.1.0",
  "description": "MobX-driven UI state classes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/soul-codes/mobx-ui-state",
  "author": "soul-codes <reach.soul.codes@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "rm -rf dist && tsc",
    "start": "concurrently --kill-others \"npm run tsc-watch\" \"npm run demo-server\"",
    "release": "release-it",
    "nightly": "fluid-publish devTag=\"inc\"",
    "tsc-watch": "rm -rf dist && tsc --watch",
    "demo-server": "webpack-serve --config ./demo/webpack.config.js --content ./demo --port 9001"
  },
  "peerDependencies": {
    "mobx": "^5.0.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/react": "^16.3.18",
    "@types/react-dom": "^16.0.6",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "fluid-publish": "^2.2.0",
    "jest": "^23.5.0",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "release-it": "^7.4.7",
    "typescript": "^3.0.1",
    "webpack": "^4.12.0",
    "webpack-serve": "^1.0.4"
  }
}
