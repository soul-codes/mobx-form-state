{
  "name": "mobx-ui-statelets",
  "version": "0.7.1",
  "description": "MobX-driven UI state classes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/soul-codes/mobx-ui-state",
  "author": "soul-codes <reach.soul.codes@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "rm -rf dist && tsc",
    "start": "concurrently --kill-others \"yarn tsc-watch\" \"yarn demo-server\"",
    "doc:install": "cd docs/jekyll && bundler install",
    "doc:ts": "typedoc --options typedoc.json src/index.ts",
    "doc:ts:watch": "nodemon --watch src --watch docs/typedoc-theme --exec \"rm -rf docs/jekyll/api && yarn doc:ts\" --delay 1 -e ts,hbs,css",
    "doc:jekyll": "jekyll build -s docs/jekyll -d docs/public",
    "doc:start": "concurrently --kill-others \"yarn doc:ts:watch\" \"yarn doc:jekyll --watch --baseurl /\" \"cd docs/public && python -m SimpleHTTPServer 8000\"",
    "doc:build": "yarn doc:install && yarn doc:ts && yarn doc:jekyll",
    "doc:deploy": "gh-pages -d docs/public",
    "release": "release-it",
    "nightly": "yarn prepare && fluid-publish devTag=\"dev\"",
    "tsc-watch": "rm -rf dist && tsc --watch",
    "demo-server": "webpack-serve --config ./demo/webpack.config.js --content ./demo --port 9001"
  },
  "peerDependencies": {
    "mobx": "^5.0.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/react": "^16.3.18",
    "@types/react-dom": "^16.0.6",
    "awesome-typescript-loader": "^5.2.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "fluid-publish": "^2.2.0",
    "gh-pages": "^2.0.0",
    "gh-pages-deploy": "^0.5.0",
    "jekyll": "^3.0.0-beta1",
    "jest": "^23.5.0",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "nodemon": "^1.18.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "release-it": "^7.4.7",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.1",
    "webpack": "^4.12.0",
    "webpack-serve": "^1.0.4"
  }
}
